<form method="post" action="{{ path('contact_delete', {'id': contact.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimmer cet item ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ contact.id) }}">
    <button class="btn btn-danger space_4">Supprimer</button>
</form>
